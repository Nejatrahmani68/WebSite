@page
@model PersonalDataModel
@{
    ViewData["Title"] = "Personal Data";
    ViewData["ActivePage"] = ManageNavPages.PersonalData;
}

<div class="row">
    <div class="col-md-12">
        زانیاریەکان
        <div class="row">
            <div class="col-md-12">
                لێرە دا تەواوی زانیاریەکانی بەرێزتان نوسراوە ،ئێوە دەتوانن وەریان بگرن یان بە تەواوی بیان سڕنەوە
                <strong>ئاگاداربە ،بە رەشکردنەوەیان توانای گەڕانەوەی بەردەست نابیت</strong>

                <hr />
                <form id="download-data" asp-page="DownloadPersonalData" method="post">
                    <button class="btn btn-primary" type="submit">وەرگرتن</button>
                    <a id="delete" asp-page="DeletePersonalData" class="btn btn-danger">سڕینەوە</a>
                </form>
            </div>
        </div>

    </div>
</div>


@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
