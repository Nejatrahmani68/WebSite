            <h5>دڵنیایی لە لابردنی؟</h5>
        </div>
        <div class="col-md-4">
            <form asp-action="Delete">
                <input type="hidden" asp-for="Id" />
                <input type="submit" value="لابردن" class="btn btn-danger" />
                <a class="btn btn-info" asp-route-idTemp="@Model?.Id_SectionPostStep" asp-action="Index">گەڕانەوە بۆ لیست</a>
            </form>
        </div>
