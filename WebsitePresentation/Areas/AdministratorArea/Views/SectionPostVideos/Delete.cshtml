        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.SectionPostStep)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SectionPostStep.Id)
        </dd>
        <dt class = "col-sm-2">
